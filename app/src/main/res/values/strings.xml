    <!-- ABOUT SCREEN -->
    <!-- Text is shown on the action bar for the page which shows app information -->
    <string name="action_bar_about_header">About</string>
    <!-- Content describes the app in detail -->
    <string-array name="about_screen_content">
        <item>Activity Art helps you create customizable artwork with your Strava activities. The art you can make with this app is perfect for framing, computer wallpaper, printing on a shirt, and more!</item>
        <item>This app is my project, and I want to make it the best I can! Please consider leaving a review if you are enjoying the app to help others find it. If you have any feature requests or find a bug, write me at tyler@activityartapp.com - thanks!</item>
    </string-array>

    <!-- LOADING ACTIVITIES SCREEN -->
    <string name="loading_activities_zero_count">
        Loading Activities
    </string>
    <!-- Text informs the athlete how many activities have been loaded -->
    <plurals name="loading_activities_count">
        <item quantity="zero">@string/loading_activities_zero_count</item>
        <item quantity="one">%d Activity Loaded</item>
        <item quantity="other">%d Activities Loaded</item>
    </plurals>
    <string name="loading_activities_description">We\'re getting your activities from Strava</string>
    <string name="loaded_activities_description">Let\'s start making Activity Art!</string>
    <string name="loading_activities_error_header">Oops! An Error Occurred</string>
    <!-- If there was no internet connection while loading activities, present the user a prompt
    depending on how many activities were loaded to either continue with the partial load or to retry -->
    <string name="loading_activities_no_internet_header">Internet is Unavailable</string>
    <string name="loading_activities_no_internet_description">We can\'t connect to the internet.</string>
    <string name="loading_activities_no_internet_prompt_zero_count">Check your connection and try again.</string>
    <plurals name="loading_activities_no_internet_prompt">
        <item quantity="zero">@string/loading_activities_no_internet_prompt_zero_count</item>
        <item quantity="one">We successfully loaded an activity. You can continue to make Activity Art with it or check your connection and try again.</item>
        <item quantity="other">We successfully loaded %d activities. You can continue to make Activity Art with them or check your connection and try again.</item>
    </plurals>
    <!-- Error information shown when the athlete has been personally rate-limited -->
    <string name="loading_activities_athlete_rate_limited_header">Personal Daily Limit</string>
    <string name="loading_activities_athlete_rate_limited_description">
        Wow, you must have a lot of activities! Because Strava limits how many times we can get athlete data in a day, we can\'t load more today.
    </string>
    <string name="loading_activities_athlete_rate_limited_prompt_zero_count">Check back tomorrow when we can get the rest.</string>
    <plurals name="loading_activities_athlete_rate_limited_prompt">
        <item quantity="zero">@string/loading_activities_athlete_rate_limited_prompt_zero_count</item>
        <item quantity="one">We successfully loaded an activity. You can continue to make Activity Art with it or check back tomorrow when we can get the rest.</item>
        <item quantity="other">We successfully loaded %d activities. You can continue to make Activity Art with them or check back tomorrow when we can get the rest.</item>
    </plurals>
    <!-- Error information shown when the app has been rate-limited by Strava -->
    <string name="loading_activities_app_rate_limited_header">Daily Limit</string>
    <string name="loading_activities_app_rate_limited_description">Activity Art is busy today! Because Strava limits how many times we can get athlete data in a day, we can\'t load any more today.</string>
    <string name="loading_activities_app_rate_limited_prompt_zero_count">Check back tomorrow when we can get the rest.</string>
    <plurals name="loading_activities_app_rate_limited_prompt">
        <item quantity="zero">@string/loading_activities_app_rate_limited_prompt_zero_count</item>
        <item quantity="one">We successfully loaded an activity. You can continue to make Activity Art with it or check back tomorrow when we can get the rest.</item>
        <item quantity="other">We successfully loaded %d activities. You can continue to make Activity Art with them or check back tomorrow when we can get the rest.</item>
    </plurals>
    <!-- Error information shown when we Strava is having Server issues -->
    <string name="loading_activities_server_issue_header">Strava Server Error</string>
    <string name="loading_activities_server_issue_description">We had a problem connecting to Strava.</string>
    <string name="loading_activities_server_issue_prompt_zero_count">Check back later or retry the connection.</string>
    <plurals name="loading_activities_server_issue_prompt">
        <item quantity="zero">@string/loading_activities_server_issue_prompt_zero_count</item>
        <item quantity="one">We successfully loaded an activity. You can continue to make Activity Art with it, retry the connection, or check back later.</item>
        <item quantity="other">We successfully loaded %d activities. You can continue to make Activity Art with them, retry the connection, or check back later.</item>
    </plurals>
    <!-- Error information shown when we don't know why we are having issues -->
    <string name="loading_activities_unknown_header">Unknown Error</string>
    <string name="loading_activities_unknown_description">We\'re not sure why, but we aren\'t able to get all of your activities right now.</string>
    <string name="loading_activities_unknown_prompt_zero_count">Check back later.</string>
    <plurals name="loading_activities_unknown_prompt">
        <item quantity="zero">@string/loading_activities_unknown_prompt_zero_count</item>
        <item quantity="one">We successfully loaded an activity. You can continue to make Activity Art with it or check back later.</item>
        <item quantity="other">We successfully loaded %d activities. You can continue to make Activity Art with them or check back later.</item>
    </plurals>
    <!-- Error information when the athlete is deauthorized -->
    <string name="loading_activities_unauthorized_header">Unauthorized Error</string>
    <string name="loading_activities_unauthorized_description">Strava has said we\'re not currently authorized to read your activities.</string>
    <string name="loading_activities_unauthorized_prompt_zero_count">Please reconnect with Strava to fix this error.</string>
    <plurals name="loading_activities_unauthorized_prompt">
        <item quantity="zero">@string/loading_activities_unauthorized_prompt_zero_count</item>
        <item quantity="one">We successfully loaded an activity. You can continue to make Activity Art with it or reconnect with Strava to fix the error.</item>
        <item quantity="other">We successfully loaded %d activities. You can continue to make Activity Art with them or reconnect with Strava to fix the error.</item>
    </plurals>
